{
  "$schema": "http://json.schemastore.org/template",
  "author": "AvaloniaUI,Nukepayload2",
  "classifications": [ "Desktop", "Xaml", "Avalonia", "Windows", "Linux", "macOS" ],
  "defaultName": "UserControl1",
  "description": "An Avalonia UI UserControl",
  "groupidentity": "Avalonia.UserControl.VB",
  "identity": "Avalonia.UserControl.VB",
  "name": "Avalonia UserControl",
  "primaryOutputs": [
    { "path": "NewUserControl.axaml.vb" },
    { "path": "NewUserControl.axaml" }
  ],
  "shortName": "avalonia.usercontrol.vb",
  "sourceName": "NewUserControl",
  "symbols": {
    "DefaultNamespace": {
      "type": "bind",
      "binding": "msbuild:RootNamespace"
    },
    "NamespaceReplacer": {
      "type": "generated",
      "generator": "coalesce",
      "parameters": {
        "sourceVariableName": "DefaultNamespace",
        "fallbackVariableName": "DefaultNamespace"
      },
      "replaces": "AvaloniaAppTemplate.Namespace"
    }
  },
  "tags": {
    "language": "VB",
    "type": "item"
  },
  "postActions": [
    {
      "id": "editor",
      "condition": "(HostIdentifier != \"dotnetcli\" && HostIdentifier != \"dotnetcli-preview\")",
      "description": "Opens UserControl in the editor.",
      "manualInstructions": [],
      "actionId": "84C0DA21-51C8-4541-9940-6CA19AF04EE6",
      "args": {
        "files": "0;1"
      },
      "continueOnError": true
    }
  ]
}